{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "moduleResolution": "node",
    "jsx": "react",
    "skipLibCheck": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "allowJs": false,
    "downlevelIteration": true,
    "lib": [
      "dom",
      "es2015",
      "es2016"
    ],
    "baseUrl": "src",
    "paths": {
      "app": [
        "app"
      ],
      "app/*": [
        "app/*"
      ],
      "contents/*": [
        "contents/*"
      ],
      "messages/*": [
        "messages/*"
      ],
      "ssenkit.autocomplete-text-input": [
        "library/ssenkit.autocomplete-text-input"
      ],
      "ssenkit.date-select": [
        "library/ssenkit.date-select"
      ],
      "ssenkit.dropdown-anchor": [
        "library/ssenkit.dropdown-anchor"
      ],
      "ssenkit.modal": [
        "library/ssenkit.modal"
      ],
      "ssenkit.restricted-text-input": [
        "library/ssenkit.restricted-text-input"
      ]
    }
  }
}