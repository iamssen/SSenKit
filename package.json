{
  "name": "ssenkit",
  "version": "2018.4.0",
  "description": "SSen's toolkits",
  "scripts": {
    "libs.build": "ssenpack libs.build",
    "libs.publish": "sh scripts.sh libs.publish",
    "messages.build": "ssenpack messages.build",
    "web.build": "ssenpack web.build",
    "web.dev.build": "ssenpack web.dev.build",
    "web.dev.start": "ssenpack web.dev.start",
    "web.server.build": "ssenpack web.server.build",
    "web.server.dev.build": "ssenpack web.server.dev.build",
    "web.server.dev.build.watch": "ssenpack web.server.dev.build.watch",
    "web.server.dev.start": "ssenpack web.server.dev.start",
    "build": "npm run messages.build && npm run web.build && npm run web.server.build && npm run libs.build",
    "start": "npm run messages.build && sh scripts.sh start",
    "test": "jest",
    "coverage": "jest --coverage",
    "web.git.publish": "git subtree push --prefix dist/web origin subtree/web",
    "web.server.git.publish": "git subtree push --prefix dist/server origin subtree/server",
    "git.publish": "npm run web.git.publish && npm run web.server.git.publish",
    "lint": "tslint 'src/**/!(*.spec|*.test).ts?(x)'",
    "download-resources": "sh scripts.sh download-resources"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamssen/ssenkit.git"
  },
  "upstream": {
    "git": "https://github.com/iamssen/seed.git",
    "hash": "cba4f46417334a0834e7939a47ac1e44cc40d9a7"
  },
  "bugs": {
    "url": "https://github.com/iamssen/ssenkit/issues"
  },
  "homepage": "https://github.com/iamssen/ssenkit",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.1",
    "@fortawesome/free-solid-svg-icons": "^5.1.1",
    "@fortawesome/react-fontawesome": "^0.1.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.5.7",
    "d3-array": "^1.2.0",
    "d3-selection": "^1.3.0",
    "deep-assign": "^2.0.0",
    "express": "^4.16.3",
    "ie-shim": "^0.1.0",
    "immer": "^1.3.1",
    "immutable": "4.0.0-rc.2",
    "js-cookie": "^2.2.0",
    "luxon": "^1.3.1",
    "normalize.css": "^8.0.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-intl": "^2.4.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "url-search-params-polyfill": "^4.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/core-js": "^2.5.0",
    "@types/d3-array": "^1.2.1",
    "@types/d3-selection": "^1.3.1",
    "@types/deep-assign": "^0.1.1",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.0",
    "@types/jquery": "^3.3.4",
    "@types/jqueryui": "^1.12.2",
    "@types/js-cookie": "^2.1.0",
    "@types/loader-utils": "^1.1.3",
    "@types/luxon": "^1.2.1",
    "@types/numeral": "0.0.23",
    "@types/prop-types": "^15.5.3",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-intl": "^2.3.8",
    "@types/react-router": "^4.0.29",
    "@types/react-router-dom": "^4.2.7",
    "@types/webpack-env": "^1.13.6",
    "electron": "^2.0.5",
    "jest": "^23.4.1",
    "pre-commit": "^1.2.2",
    "ssenpack": "^2018.4.2",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "pre-commit": [
    "lint"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src",
      "src/_library"
    ],
    "coverageDirectory": "coverage"
  }
}
