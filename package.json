{
  "name": "ssenkit",
  "version": "2019.1.0",
  "description": "SSen's modules and components",
  "scripts": {
    "module.build": "zeroconfig module.build",
    "module.publish": "zeroconfig module.publish",
    "web.build": "zeroconfig web.build",
    "web.dev.build": "zeroconfig web.dev.build",
    "web.dev.build.watch": "zeroconfig web.dev.build.watch",
    "web.dev.start": "zeroconfig web.dev.start",
    "web.ssr.build": "zeroconfig web.ssr.build",
    "web.ssr.dev.build": "zeroconfig web.ssr.dev.build",
    "web.ssr.dev.build.watch": "zeroconfig web.ssr.dev.build.watch",
    "web.ssr.dev.start": "zeroconfig web.ssr.dev.start",
    "translation.build": "zeroconfig intl.build",
    "translation.build.watch": "zeroconfig intl.build.watch",
    "translation.distribute": "zeroconfig intl.distribute",
    "template.build": "zeroconfig template.build",
    "resource.download": "node ./resource.download",
    "generate": "npm run resource.download && npm run translation.build && npm run template.build",
    "build": "npm run generate && npm run web.build && npm run web.ssr.build && npm run module.build",
    "start": "npm run generate && multiplerun development",
    "start-translation": "npm run generate && multiplerun translation",
    "test": "jest",
    "lint": "tslint \"src/**/!(*.spec|*.test).ts?(x)\"",
    "git.publish.web": "git subtree push --prefix dist/web origin dist/web",
    "git.publish.ssr": "git subtree push --prefix dist/ssr origin dist/ssr",
    "git.publish.ghpages": "git subtree push --prefix dist/web origin gh-pages",
    "git.publish": "npm run git.publish.web && npm run git.publish.ssr && npm run git.publish.ghpages"
  },
  "multiplerun": {
    "development": [
      [
        "npm run web.ssr.dev.build.watch",
        "npm run web.ssr.dev.start"
      ],
      "npm run web.dev.start"
    ],
    "translation": [
      [
        "npm run web.ssr.dev.build.watch",
        "npm run web.ssr.dev.start"
      ],
      [
        "npm run web.dev.start",
        "npm run translation.build.watch"
      ]
    ]
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamssen/ssenkit.git"
  },
  "bugs": {
    "url": "https://github.com/iamssen/ssenkit/issues"
  },
  "homepage": "https://github.com/iamssen/ssenkit",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "d3-array": "^2.0.2",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "history": "^4.7.2",
    "immer": "^1.10.5",
    "js-cookie": "^2.2.0",
    "moment": "^2.23.0",
    "moment-timezone": "^0.5.23",
    "node-fetch": "^2.3.0",
    "numeral": "^2.0.6",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.0-alpha.0",
    "react-app-polyfill": "^0.2.0",
    "react-dom": "^16.8.0-alpha.0",
    "react-intl": "^2.8.0",
    "react-markdown": "^4.0.6",
    "react-router": "^4.4.0-beta.6",
    "react-router-dom": "^4.4.0-beta.6",
    "url-search-params-polyfill": "^5.0.0",
    "use-react-router": "^1.0.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/core-js": "^2.5.0",
    "@types/d3-array": "^1.2.4",
    "@types/ejs": "^2.6.1",
    "@types/express": "^4.16.0",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.13",
    "@types/js-cookie": "^2.2.0",
    "@types/loader-utils": "^1.1.3",
    "@types/mkdirp": "^0.5.2",
    "@types/moment-timezone": "^0.5.9",
    "@types/node-fetch": "^2.1.4",
    "@types/numeral": "0.0.25",
    "@types/prismjs": "^1.9.0",
    "@types/prop-types": "^15.5.8",
    "@types/react": "^16.7.21",
    "@types/react-dom": "^16.0.11",
    "@types/react-intl": "^2.3.15",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/webpack-env": "^1.13.6",
    "jest": "^24.0.0",
    "mkdirp": "^0.5.1",
    "multiplerun": "^0.1.4",
    "pre-commit": "^1.2.2",
    "react-zeroconfig": "^1.3.6",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.1",
    "tslint-ssen-rules": "^1.0.0",
    "typescript": "^3.2.4"
  },
  "pre-commit": [
    "test",
    "lint"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/_modules",
      "src"
    ]
  }
}
