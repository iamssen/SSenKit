{
  "name": "seed",
  "version": "2017.4.0",
  "description": "{project-description}",
  "scripts": {
    "build:web": "webpack --env.action=build:web",
    "build:web:dev": "webpack --env.action=build:web:dev",
    "build:web:dev:dll": "webpack --env.action=build:web:dev:dll",
    "watch:web:dev": "webpack --watch --env.action=build:web:dev",
    "build:server": "webpack --env.action=build:server",
    "build:server:dev": "webpack --env.action=build:server:dev",
    "watch:server:dev": "webpack --watch --env.action=build:server:dev",
    "build:libs": "sh scripts.sh build:libs",
    "publish:libs": "sh scripts.sh publish:libs",
    "nodemon": "node nodemon.js",
    "serve": "node browser-sync.js",
    "build": "npm run build:web && npm run build:server && npm run build:libs",
    "start": "sh scripts.sh start",
    "test": "jest",
    "coverage": "jest --coverage",
    "subtree:web": "git subtree push --prefix dist/web origin subtree/web",
    "subtree:server": "git subtree push --prefix dist/server origin subtree/server",
    "subtree": "npm run subtree:web && npm run subtree:server",
    "lint": "tslint 'src/**/!(*.spec|*.test).ts?(x)'",
    "build:messages": "node src/messages/build-messages.js",
    "build:electron:dev": "webpack --env.action=build:electron:dev",
    "watch:electron:dev": "webpack --watch --env.action=build:electron:dev",
    "electron": "electron start-electron.js",
    "start:electron": "sh scripts.sh start:electron"
